{
    "version": "1",
    "inputs": [
        {
            "name": "prompt",
            "description": "The prompt to feed to the model.",
            "type": "string",
            "required": true,
            "default": "An astronaut riding a green horse"
        },
        {
            "name": "height",
            "description": "Height of the image",
            "type": "number",
            "default": 1024
        },
        {
            "name": "width",
            "description": "Width of the image",
            "type": "number",
            "default": 1024
        }
    ],
    "output": {
        "image": {
            "type": "image",
            "source": "base64",
            "path": "output"
        }
    }
}